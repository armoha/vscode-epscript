{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "out",
    "rootDir": "src",
    "lib": ["es6"]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
