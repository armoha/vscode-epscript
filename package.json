{
    "name": "epscript",
    "displayName": "epScript",
    "description": "Syntax highlighter and snippets for epScript",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "epscript",
                "aliases": [
                    "epScript",
                    "epscript",
                    "eps"
                ],
                "extensions": [
                    ".eps"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "epscript",
                "scopeName": "source.eps",
                "path": "./syntaxes/epScript.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "epscript",
                "path": "./snippets/epscript.json"
            }
        ]
    }
}
